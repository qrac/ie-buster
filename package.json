{
  "name": "ie-buster",
  "description": "Save web creators with Chrome link popup for IE.",
  "version": "2.0.0",
  "cdn": "dist/ie-buster.js",
  "cdn:min": "dist/ie-buster.min.js",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "node": "./lib/esm/index.js",
    "default": "./lib/cjs/index.js"
  },
  "files": [
    "dist",
    "lib",
    "types"
  ],
  "license": "MIT",
  "homepage": "https://ie-buster.qranoko.jp",
  "repository": "https://github.com/qrac/ie-buster",
  "bugs": "https://github.com/qrac/ie-buster/issues",
  "keywords": [
    "javascript",
    "ie",
    "ie11",
    "japanese"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "illustration": {
    "name": "Mainoko",
    "url": "https://mainoko.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "prettier": {
    "semi": false
  },
  "browserslist": [
    "ie 11"
  ],
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/plugin-transform-object-assign"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@rollup/plugin-babel": "^5.3.0",
    "rollup": "^2.39.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
